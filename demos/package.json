{
  "name": "demos",
  "private": true,
  "scripts": {
    "serve": "npm-run-all --parallel serve:*",
    "serve:hotel": "cd hotel-demo-1 && npm run serve",
    "serve:restaurant": "cd restaurant-demo-1 && npm run serve",
    "serve:bus-tour": "cd bus-tour-demo-1 && npm run serve",
    "postinstall": "cd hotel-demo-1 && npm install && cd ../restaurant-demo-1 && npm install && cd ../bus-tour-demo-1 && npm install"
  },
  "devDependencies": {
    "http-server": "^14.1.1",
    "npm-run-all": "^4.1.5"
  }
}
